<process name="order_card">
  <start name="start" >
    <transition to="order_create"/>
  </start>
 
  <node name="order_create"  classname="com.tmall.asshole.mock.ep.TestEvent1"  scheduleType="BASIC_SCHEDULE">
    <transitions>
      <transition to="order_execute"  exp="$a==$b"/>
    </transitions>
  </node>
  
  <node name="order_execute" classname="com.tmall.asshole.mock.ep.TestEvent2"  scheduleType="DELAY_SCHEDULE">
    <transitions>
      <transition to="end"/>
    </transitions>
  </node>
  
  <end name="end"/>
</process>